{
    "name": "abdallahmohammed/alsaad-laravel",
    "description": "Laravel Package for Alsaad2's PHP Client",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "AbdallahMohammed",
            "email": "abdallah.r660@gmail.com",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^5.6|^7.1",
        "illuminate/support": "^5.2|^6.0|^7.0",
        "abdallahmohammed/alsaad-php": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.3|~6.0|~8.0",
        "orchestra/testbench": "~3.0|^4.0|^5.0"
    },
    "autoload": {
        "psr-4": {
            "Alsaad\\Laravel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Alsaad\\Laravel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Alsaad\\Laravel\\AlsaadServiceProvider"
            ],
            "aliases": {
                "Alsaad": "Alsaad\\Laravel\\Facade\\Alsaad"
            }
        }
    }
}
